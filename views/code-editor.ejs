<%-include('partials/navbar.ejs')-%>
    <br>
    <h1 id="compiler-name"><%=lang%> code editor IDE</h1>
    <form action="/code-editor" method="post">
        <div id="flexer">
            <textarea id="message" rows="4"
                class=" code-editor block p-2.5 w text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 focus:ring-blue-500 focus:border-blue-500 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500"
                name="code" placeholder="<%=code%>"></textarea>
                <button type="submit" class="btnhu text-gray-900 bg-gradient-to-r from-lime-200 via-lime-400 to-lime-500 hover:bg-gradient-to-br focus:ring-4 focus:outline-none focus:ring-lime-300 dark:focus:ring-lime-800 font-medium rounded-lg text-sm px-5 py-2.5 text-center mr-2 mb-2">Compile</button>
            <div
                class="code-editor block p-2.5 w text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 focus:ring-blue-500 focus:border-blue-500 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500">
            <h3><%=output%></h3>
            </div>
        </div>
    </form>
    <br>
    <%-include('partials/footer.ejs')-%>